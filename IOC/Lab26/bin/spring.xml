<?xml version="1.0" encoding="UTF-8"?>
<beans  
    xmlns="http://www.springframework.org/schema/beans"  
    xmlns:p="http://www.springframework.org/schema/p"  
    xmlns:context="http://www.springframework.org/schema/context"  
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  
    xsi:schemaLocation="http://www.springframework.org/schema/beans  
    http://www.springframework.org/schema/beans/spring-beans-4.2.xsd
    http://www.springframework.org/schema/context  
    http://www.springframework.org/schema/context/spring-context-4.2.xsd">  
    
    <context:annotation-config/>
    
	<bean id="aobj1" class="com.avishek.spring.A" p:a="102" p:msg="Debais"/>
	<bean id="aobj2" class="com.avishek.spring.A" p:a="105" p:msg="XXXXXX"/>
		
    
	<bean id="bobj1" class="com.avishek.spring.B">
		<constructor-arg value="101"/>
		<constructor-arg value="Avishek"/>
	</bean>
	
	<bean id="bobj2" class="com.avishek.spring.B">
		<constructor-arg value="103"/>
		<constructor-arg value="YYYYYYYY"/>
	</bean>

	<bean id="hello" class="com.avishek.spring.Hello"/>
	
</beans>