package com.avishek.spring;

public class B {
	private int b;
	private String str;
	

	static{
		System.out.println("Load B Bean");
	}
	
	public B(int b, String str) {
		System.out.println("Class B 2-arg Constructor");
		this.b = b;
		this.str = str;
	}
	
	public void showB()
	{
		System.out.println(b);
		System.out.println(str);
	}
}
