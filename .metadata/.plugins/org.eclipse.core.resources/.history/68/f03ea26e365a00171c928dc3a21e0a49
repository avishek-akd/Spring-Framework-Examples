package com.avishek.spring;

import java.util.List;

public class Customer {
	private int cid;
	private String cname;
	private String email;
	private Long phones;
	
	private List<String> emails;
	
	public Customer(int cid, String cname, String email, Long phones) {
		this.cid = cid;
		this.cname = cname;
		this.email = email;
		this.phones = phones;
	}
	
	
	
	
	
	
	public void setEmails(List<String> emails) {
		this.emails = emails;
	}






	public void show(){
		System.out.println(cid + "\t" + cname + "\t" + email+ "\t" +phones);
		
		System.out.println(emails);
		System.out.println(emails.getClass().getSimpleName());
	}
}
